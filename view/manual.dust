<!-- Custom JS -->
<script src="js/custom.js"></script>

<script type="text/javascript">
    function getLocation(){
        if (navigator.geolocation) {
            navigator.geolocation.getCurrentPosition(initPosition, error, {
                /* 指示浏览器获取高精度的位置，默认为false */
                enableHighAccuracy: true,
                /* 指定获取地理位置的超时时间，默认不限时，单位为毫秒 */
                timeout: 5000,
                /* 最长有效期，在重复获取地理位置时，此参数指定多久再次获取位置。 */
                maximumAge: 3000
            });
        }
    }

    getLocation();

    function initPosition(position) {
        document.getElementById('myFrame_location').contentWindow.location.reload(true);
        $('#myFrame_controlbox').attr('src', '/manual_location.html?lon=' + position.coords.longitude + '&lat=' + position.coords.latitude);
    }

    function error(err){
    }

    var c = $("#course");

    function change_course() {
        document.getElementById('myFrame_controlbox').contentWindow.location.reload(true);
        $('#myFrame_controlbox').attr('src', '/manual_controlbox.html?course_id=' + c.val());
        document.getElementById('myFrame_area').contentWindow.location.reload(true);
        $('#myFrame_area').attr('src', '/manual_area.html?course_id=' + c.val());
    }

    jQuery(function ($) {
        /* 刷新 */
        $("#refresh").click(function() {
            getLocation();
            change_course();
        });
    });
</script>
