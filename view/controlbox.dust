                                </div>
                            </div>
                            <div class="col-lg-2 col-md-4 col-sm-6 col-xs-6">
                                <div class="form-group">
                                    <label for="controlbox_model">设备型号</label>
                                    <select class="form-control" id="controlbox_model" name="controlbox_model">
{#controlbox_models}
