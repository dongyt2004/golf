<!-- Custom JS -->
<script src="js/custom.js"></script>

<script type="text/javascript">
    /* 转移喷头形状 */
    function transfer_shape(id) {
        $('#shape_id').val(id);
        $('#shape_name').val($('td.shape_name_' + id).html());
    }

    jQuery(function($) {
        var s = $("#shape_name");
        var si = $("#shape_id");

        /* 新增喷头形状 */
        $("#c_shape").click(function() {
            if (s.val() === '') {
                alertify.error("名称不能为空");
                return;
            }
            $.post("/add_shape.do", {
                name: s.val()
            }, function (result) {
                if (result.success) {
                    window.location.href = '/nozzle_shape.html';
                }
            });
        });

        /* 修改喷头形状 */
        $("#u_shape").click(function() {
            if (si.val() === '') {
                alertify.error("请先单击行，选择要修改的形状");
                return;
            }
            if (s.val() === '') {
                alertify.error("名称不能为空");
                return;
            }
            $.post("/update_shape.do", {
                id: si.val(),
                name: s.val()
            }, function (result) {
                if (result.success) {
                    window.location.href = '/nozzle_shape.html';
                }
            });
        });
    });
</script>
</body>
